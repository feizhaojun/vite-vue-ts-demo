<?xml version="1.0" encoding="UTF-8"?>
<page
  xmlns=""
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation=""
  width="210"
  height="285"
  splitable="false"
>
  <layout id="doudian-template-1" width="42" height="5" left="2" top="9" style="">
    <text style="fontFamily:黑体;fontSize:8;">
      <![CDATA[头部信息]]>
    </text>
  </layout>
  
  <table id="doudian-template-2" width="210" height="320" left="0" top="18" zIndex="0">
    <tr>
      <td width="20" height="10">
        <text style="fontFamily:黑体;fontSize:8;">
          <![CDATA[一一一]]>
        </text>
      </td>
      <td width="20" height="10">
        <text style="fontFamily:黑体;fontSize:8;">
          <![CDATA[二二二]]>
        </text>
      </td>
      <td width="20" height="10">
        <text style="fontFamily:黑体;fontSize:8;">
          <![CDATA[三三三]]>
        </text>
      </td>
    </tr>
    <% if(_data.name) { %>
      <% for (var i=0;i<_data.name.length;i++) {%>
        <tr>
        <td width="20" height="10">
          <text style="fontFamily:黑体;fontSize:8;">
            <![CDATA[<%=_data.name[i].col1%>]]>
          </text>
        </td>
        <td width="20" height="10">
          <text style="fontFamily:黑体;fontSize:8;">
          <![CDATA[<%=_data.name[i].col2%>]]>
          </text>
        </td>
        <td width="20" height="10">
          <text style="fontFamily:黑体;fontSize:8;">
          <![CDATA[<%=_data.name[i].col3%>]]>
          </text>
        </td>
        </tr>
      <% } %>
    <% } %>
  </table>
</page>